mixin committeeOnlineCount
        .col-sm-6.col-md-4.col-lg-3
            a.boxItem.user.neutral
                .number= users.length
                .name Total
        .col-sm-6.col-md-4.col-lg-3
            a.boxItem.user.online
                .number= onlineCount
                .name Online
        .col-sm-6.col-md-4.col-lg-3
            a.boxItem.user.offline
                .number= offlineCount
                .name Offline
