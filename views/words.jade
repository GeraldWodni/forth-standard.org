extends layout

block menu
    +menu

block content
    +box-document
        h1 Wordsets
        ul.row.boxList
            each wordSet in standard.wordSets
                li.col-xs-4
                    a(href="/standard/#{wordSet.name}").boxItem
                        +fa("cube").icon
                        .name
                            = wordSet.name
